<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>{% block title %}BLOG{% endblock %}</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    {% stylesheets
        'bundles/backend/template/bootstrap/css/bootstrap.min.css'
        'bundles/backend/vendor/font-awesome-4.6.1/css/font-awesome.min.css'
        'bundles/backend/vendor/ionicons-2.0.1/css/ionicons.min.css'
        'bundles/backend/template/plugins/datatables/dataTables.bootstrap.css'
        'bundles/backend/template/plugins/morris/morris.css'
        '@BackendBundle/Resources/public/vendor/bootstrap-datetimepicker/bootstrap-datetimepicker.css'
        'bundles/backend/vendor/bootstrap-tagsinput-latest/dist/bootstrap-tagsinput.less'
        'bundles/backend/template/dist/css/AdminLTE.min.css'
        '@BackendBundle/Resources/public/template/dist/css/skins/skin-black.min.css'
        'bundles/backend/vendor/DataTables/datatables.min.css'
        '@BackendBundle/Resources/public/css/general.less'
    filter='cssrewrite' filter='?uglifycss'
    %}
    <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}

    {% block stylesheets %}{% endblock %}
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
</head>
<body class="{% block body_classes %} hold-transition skin-black sidebar-mini {% endblock body_classes %}">
{% block fos_wrapper %}
    <!-- Site wrapper -->
    <div class="wrapper">
        <header class="main-header">
            <!-- Logo -->
            <a href="#" class="logo">
                <!-- mini logo for sidebar mini 50x50 pixels -->
                <span class="logo-mini"><b>BLOG</b></span>
                <!-- logo for regular state and mobile devices -->
                <span class="logo-lg"><b>BLOG</b></span>
            </a>
            <!-- Header Navbar: style can be found in header.less -->
            <nav class="navbar navbar-static-top">
                <!-- Sidebar toggle button-->
                <a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </a>
                <div class="navbar-custom-menu">
                    <ul class="nav navbar-nav">
                        {% if app.user %}
                            <!-- User Account: style can be found in dropdown.less -->
                            <li class="dropdown user user-menu">
                                <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                    <img src="{{ asset('img/user.png') }}" class="user-image" alt="User Image">
                                    <span class="hidden-xs">{{ app.user }}</span>
                                </a>
                                <ul class="dropdown-menu">
                                    <!-- User image -->
                                    <li class="user-header">
                                        <img src="{{ asset('img/user.png') }}" class="img-circle" alt="User Image">

                                        <p>
                                            {{ app.user }}
                                            <small>{{ app.user }}</small>
                                        </p>
                                    </li>
                                    <!-- Menu Footer-->
                                    <li class="user-footer">
                                        <div class="pull-left">
                                            <a href="#" class="btn btn-default btn-flat">Mi perfil</a>
                                        </div>
                                        <div class="pull-right">
                                            <a href="#" class="btn btn-default btn-flat">Salir</a>
                                        </div>
                                    </li>
                                </ul>
                            </li>
                        {% endif %}
                    </ul>
                </div>
            </nav>
        </header>
        <!-- =============================================== -->
        <!-- Left side column. contains the sidebar -->
        <aside class="main-sidebar">
            <!-- sidebar: style can be found in sidebar.less -->
            <section class="sidebar">
                <!-- sidebar menu: : style can be found in sidebar.less -->
                <ul class="sidebar-menu">
                    <li class="start-logo">
                        {#<img src="{{ asset('img/logo.png') }}" alt="logo" class="logo-default" width="100%"/>#}
                    </li>
                    <li class="header">MENÃš PRINCIPAL</li>
                    <li><a href="{{ path('backend_blog_index') }}"><i class="fa fa-check-square-o" aria-hidden="true"></i> <span></span></a></li>
                </ul>
            </section>
            <!-- /.sidebar -->
        </aside>
        <!-- =============================================== -->
        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            {% block body %}{% endblock body %}
        </div>
        <!-- /.content-wrapper -->
        <footer class="main-footer">
            <div class="pull-right hidden-xs">
                <b>Version</b> 1.0
            </div>
            <strong>Copyright &copy; {{ "now"|date("Y") }}  <a href="#" target="_blank">Blogs</a>.</strong> All rights reserved.
        </footer>
    </div>
    <!-- ./wrapper -->
{% endblock fos_wrapper %}

{#'bundles/fosjsrouting/js/router.js'#}
{#'js/fos_js_routes.js'#}
{% javascripts
    '@BackendBundle/Resources/public/template/plugins/jQuery/jQuery-2.2.0.min.js'
    '@BackendBundle/Resources/public/template/bootstrap/js/bootstrap.min.js'
    '@BackendBundle/Resources/public/js/lib/app.coffee'
    '@BackendBundle/Resources/public/vendor/moment/moment-with-locales.js'
    '@BackendBundle/Resources/public/template/plugins/datatables/jquery.dataTables.min.js'
    '@BackendBundle/Resources/public/template/plugins/datatables/dataTables.bootstrap.min.js'
    'bundles/a2lixtranslationform/js/a2lix_translation_bootstrap.js'
    '@BackendBundle/Resources/public/template/plugins/slimScroll/jquery.slimscroll.min.js'
    '@BackendBundle/Resources/public/vendor/jquery-validation-1.15.0/dist/jquery.validate.min.js'
    '@BackendBundle/Resources/public/vendor/jquery-validation-1.15.0/dist/additional-methods.min.js'
    '@BackendBundle/Resources/public/vendor/jquery-validation-1.15.0/dist/localization/messages_es.min.js'
    '@BackendBundle/Resources/public/template/plugins/fastclick/fastclick.min.js'
    'bundles/backend/vendor/typehead/typeahead.bundle.js'
    'bundles/backend/vendor/DataTables/datatables.min.js'
    '@BackendBundle/Resources/public/template/dist/js/app.min.js'
    '@BackendBundle/Resources/public/js/customInit.coffee'
filter='?uglifyjs2'
%}
<script src="{{ asset_url }}"></script>
{% endjavascripts %}

{% block javascripts %}{% endblock %}
</body>
</html>